  <xs:complexType name="IPAddresses" >
    <xs:sequence>
      <xs:element name="Addresses" type="IP4Type" minOccurs="0" maxOccurs="unbounded"></xs:element>
    </xs:sequence>
  </xs:complexType>
  
  
  <xs:simpleType name="IP4Type">
